From 59f9281c671f7580f3bb78d2ed504b97c7777048 Mon Sep 17 00:00:00 2001
From: Henrik Pihl <ahvenas@gmail.com>
Date: Sat, 8 Nov 2014 11:15:58 +0200
Subject: [PATCH] Minor fixes to the library names, paths, and don't strip
 files.

---
 zeal/zeal.pro               | 8 ++++----
 zeal/zealsettingsdialog.cpp | 4 ++--
 2 files changed, 6 insertions(+), 6 deletions(-)

diff --git a/zeal/zeal.pro b/zeal/zeal.pro
index 01546ef..accd6fd 100644
--- a/zeal/zeal.pro
+++ b/zeal/zeal.pro
@@ -65,17 +65,18 @@ LIBS += -lz -L/usr/lib
 
 CONFIG += link_pkgconfig
 
-unix:!macx: LIBS += -lxcb -lxcb-keysyms
+unix:!macx: LIBS += -lxcb -lxcb-keysyms -lquazip
 unix:!macx: SOURCES += xcb_keysym.cpp
 unix:!macx: DEFINES += LINUX
 unix:!macx: QMAKE_DEL_DIR = rmdir --ignore-fail-on-non-empty
+unix:!macx: QMAKE_STRIP = echo
 
 unix:!macx:!no_libappindicator {
-    INCLUDEPATH += /usr/include/libappindicator-0.1 \
+    INCLUDEPATH += /usr/include/libappindicator3-0.1 \
         /usr/include/gtk-2.0 \
         /usr/lib/gtk-2.0/include
     PKGCONFIG = gtk+-2.0
-    LIBS += -lappindicator
+    LIBS += -lappindicator3
 
     DEFINES += USE_LIBAPPINDICATOR
 }
@@ -97,7 +98,6 @@ desktop.files=zeal.desktop
 unix:INSTALLS += appicons16 appicons24 appicons32 appicons64 appicons128 icons desktop
 
 include (widgets/widgets.pri)
-include (quazip/quazip.pri)
 
 RESOURCES += \
     zeal.qrc
diff --git a/zeal/zealsettingsdialog.cpp b/zeal/zealsettingsdialog.cpp
index 8cce8b5..75bdf41 100644
--- a/zeal/zealsettingsdialog.cpp
+++ b/zeal/zealsettingsdialog.cpp
@@ -14,8 +14,8 @@
 #include <QFutureWatcher>
 #include <QFileDialog>
 #include <QtNetwork/QNetworkProxy>
-#include "quazip/quazip.h"
-#include "JlCompress.h"
+#include <quazip/quazip.h>
+#include <quazip/JlCompress.h>
 #include "progressitemdelegate.h"
 
 #include <QDebug>
-- 
2.0.4

